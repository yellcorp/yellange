{
  "name": "yellange",
  "version": "0.1.0",
  "description": "Melange of JS libraries motivated by mid-2010s frontend development",
  "repository": "github:yellcorp/yellange",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "format": "prettier --write .",
    "server": "http-server . --port 7979 -o /test/ -c-1 --no-dotfiles",
    "test": "nyc --reporter=html --reporter=text mocha",
    "lint": "eslint index.js string.js",
    "test-browser": "npm run build && npm run server"
  },
  "author": "Jim Boswell <jimb@yellcorp.org> (http://yellcorp.org/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "http-server": "^0.12.3",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "2.1.2",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  }
}
